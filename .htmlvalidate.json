{
    "extends": ["html-validate:recommended"]
} 
  
  
{
    "extends": [
      /* bundled preset */
      "html-validate:recommended",
  
      /* npm package */
      "my-npm-package",
  
      /* plugin with custom preset */
      "my-plugin:custom",
  
      /* local file */
      "./file"
    ]
}

{
    "rules": {
      "some-rules": "severity",
      "other-rule": ["severity", { "option": true }]
    }
}

{
    "elements": [
      "html5",
      "my-npm-package",
      "./file",
      {
        /* inline metadata */
      }
    ]
}

{
    "plugins": ["my-fancy-plugin", "./local-plugin"]
}

{
    "transform": {
      "^.*\\.vue$": "html-validate-vue"
    }
}

{
    "root": true
}



